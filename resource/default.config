<?xml version="1.0" encoding="UTF-8"?>
<config>
    <acceptors>
         <tcp address="0.0.0.0" port="80"/>
        <tls address="0.0.0.0" port="443" tls_version="tls1.3" cert_file="D:\tls\server.crt" key_file="D:\tls\server.key"/>
    </acceptors>
    <http>
        <resource default="/index.html">
            <rnodes>
                <filesystem virtual_path="/" physical_path="D:/web_root"/>
            </rnodes>
        </resource>
    </http>
</config>
