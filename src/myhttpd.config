<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <module name="cgi"/>
    </modules>
    <server workers="4">
        <http timeout="3000">
            <default_pages>
                <default_page>index.htm</default_page>
                <default_page>index.html</default_page>
            </default_pages>
        </http>
        <acceptors>
            <acceptor address="127.0.0.1" port="80" tls="none" certification=""/>
            <acceptor address="0.0.0.0" port="443" tls="none" certification=""/>
        </acceptors>
        <resources>
            <filesystem virtual_path="/" physical_path="D:/web_root"/>
            <forwarding virtual_path="/dynamic_pages" module="cgi"/>
        </resources>
    </server>
</config>